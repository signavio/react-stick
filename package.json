{
  "name": "react-stick",
  "version": "0.0.0-development",
  "description": "React component to stick a portaled node to an anchor node",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib/*",
    "es/*"
  ],
  "repository": "git@github.com:signavio/react-stick.git",
  "author": "Jan-Felix Schwarz <jan-felix.schwarz@signavio.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc --build tsconfig.commonjs.json && tsc --build tsconfig.es2015.json",
    "prepublishOnly": "yarn build",
    "build-demo": "webpack",
    "clean": "rimraf lib es build",
    "format": "prettier --write --no-semi --single-quote --trailing-comma es5 \"{src,tests,demo}/**/*.ts(x)\"",
    "lint": "eslint --max-warnings=0 src tests demo",
    "prenow-build": "yarn build",
    "now-build": "webpack",
    "start": "NODE_ENV=development webpack-dev-server",
    "semantic-release": "semantic-release",
    "test": "nwb test-react"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "requestidlecallback": "^0.3.0",
    "substyle": "^9.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "11.0.4",
    "@types/expect": "24.3.0",
    "@types/invariant": "2.2.34",
    "@types/mocha": "8.0.3",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/requestidlecallback": "0.3.1",
    "@types/sinon": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "condition-circle": "2.0.2",
    "core-js": "3.7.0",
    "eslint": "7.13.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "glamor": "2.20.40",
    "html-webpack-harddisk-plugin": "1.0.2",
    "html-webpack-plugin": "4.5.0",
    "inferno": "7.4.6",
    "karma-typescript": "5.2.0",
    "nwb": "0.25.2",
    "prettier": "2.1.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "regenerator-runtime": "0.13.7",
    "semantic-release": "17.2.2",
    "sinon": "9.1.0",
    "rimraf": "3.0.2",
    "substyle-glamor": "4.1.0",
    "ts-loader": "8.0.4",
    "webpack": "4.44.2",
    "typescript": "4.0.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "browserslist": [
    "chrome >= 50",
    "firefox >= 52",
    "safari >= 10",
    "ie >= 11"
  ],
  "release": {
    "branch": "master",
    "branches": [
      "master"
    ],
    "verifyConditions": "condition-circle"
  }
}
