{
  "name": "react-stick",
  "version": "0.0.0-development",
  "description": "React component to stick a portaled node to an anchor node",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": ["lib/*", "es/*"],
  "repository": "git@github.com:signavio/react-stick.git",
  "author": "Jan-Felix Schwarz <jan-felix.schwarz@signavio.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nwb build-react-component --no-demo",
    "postbuild": "flow-copy-source -v src lib && flow-copy-source -v src es",
    "prepublishOnly": "yarn build",
    "build-demo": "webpack",
    "clean": "nwb clean-module && nwb clean-demo",
    "flow": "flow check",
    "format": "prettier --write --no-semi --single-quote --trailing-comma es5 \"{src,stories}/**/*.js\"",
    "lint": "eslint --max-warnings=0 --ext .js src tests demo/src",
    "prenow-build": "yarn build",
    "now-build": "webpack",
    "start": "NODE_ENV=development webpack-dev-server",
    "semantic-release": "semantic-release",
    "semantic-release-preview": "semantic-release-github-pr",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "requestidlecallback": "^0.3.0",
    "substyle": "^9.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-flow": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "condition-circle": "2.0.2",
    "core-js": "3.6.5",
    "eslint": "7.7.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.0.8",
    "flow-bin": "0.131.0",
    "flow-copy-source": "2.0.9",
    "glamor": "2.20.40",
    "html-webpack-harddisk-plugin": "1.0.2",
    "html-webpack-plugin": "4.3.0",
    "inferno": "7.4.2",
    "nwb": "0.25.2",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "regenerator-runtime": "0.13.7",
    "semantic-release": "17.1.1",
    "substyle-glamor": "4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "browserslist": [
    "chrome >= 50",
    "firefox >= 52",
    "safari >= 10",
    "ie >= 11"
  ],
  "release": {
    "branch": "master",
    "branches": [
      "master"
    ],
    "verifyConditions": "condition-circle"
  }
}
