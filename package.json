{
  "name": "react-stick",
  "version": "0.0.0-development",
  "description": "React component to stick a portaled node to an anchor node",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib/*",
    "es/*"
  ],
  "repository": "git@github.com:signavio/react-stick.git",
  "author": "Jan-Felix Schwarz <jan-felix.schwarz@signavio.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nwb build-react-component --no-demo",
    "postbuild": "flow-copy-source -v src lib && flow-copy-source -v src es",
    "prepublishOnly": "yarn build",
    "build-demo": "webpack",
    "clean": "nwb clean-module && nwb clean-demo",
    "flow": "flow check",
    "format": "prettier --write --no-semi --single-quote --trailing-comma es5 \"{src,stories}/**/*.js\"",
    "lint": "eslint --max-warnings=0 --ext .js src tests demo/src",
    "prenow-build": "yarn build",
    "now-build": "webpack",
    "start": "NODE_ENV=development webpack server",
    "semantic-release": "semantic-release",
    "semantic-release-preview": "semantic-release-github-pr",
    "test": "nwb test-react"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "requestidlecallback": "^0.3.0",
    "substyle": "^9.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/plugin-transform-runtime": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-flow": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@testing-library/react": "12.1.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "condition-circle": "2.0.2",
    "core-js": "3.22.3",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "flow-bin": "0.133.0",
    "flow-copy-source": "2.0.9",
    "glamor": "2.20.40",
    "html-webpack-harddisk-plugin": "2.0.0",
    "html-webpack-plugin": "4.5.2",
    "inferno": "7.4.11",
    "nwb": "0.25.2",
    "prettier": "2.6.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "regenerator-runtime": "0.13.9",
    "semantic-release": "17.4.7",
    "substyle-glamor": "4.1.2",
    "webpack": "4.46.0",
    "webpack-cli": "4.9.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "browserslist": [
    "chrome >= 50",
    "firefox >= 52",
    "safari >= 10",
    "ie >= 11"
  ],
  "release": {
    "branch": "master",
    "branches": [
      "master"
    ],
    "verifyConditions": "condition-circle"
  }
}
